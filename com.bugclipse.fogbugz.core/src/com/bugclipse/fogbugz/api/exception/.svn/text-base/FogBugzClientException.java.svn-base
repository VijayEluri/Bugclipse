public class FogBugzClientException extends Exception {

	public FogBugzClientException(String string) {
		super(string);
	}
	
	public FogBugzClientException(Throwable t){
		super(t);
	}
	
	public FogBugzClientException(String s, Throwable t){
		super(s, t);
	}

}
