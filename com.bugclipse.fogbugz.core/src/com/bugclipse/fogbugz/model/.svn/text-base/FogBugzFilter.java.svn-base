package com.bugclipse.fogbugz.model;

public class FogBugzFilter {
	
	private final String name, description, url;

	public FogBugzFilter(final String name, final String description, final String url) {
		this.name = name;
		this.description = description;
		this.url = url;
	}

	public String getDescription() {
		return description;
	}

	public String getName() {
		return name;
	}

	public String getUrl() {
		return url;
	}

}
