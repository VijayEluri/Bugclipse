            <property name="dataSource">IntervalsDataSource</property>
            <method name="open"><![CDATA[intervals = Intervals;
count = 0;]]></method>
            <method name="fetch"><![CDATA[if(count == 0 && intervals.length == 0)
{
	row["title"] = "No data";
	count++;
	return true;
}
if (count >= intervals.length)
	return false;
row["title"] = intervals[count].getSTitle();
row["taskId"] = intervals[count].getIxBug();
from = intervals[count].getDtStart();
to = intervals[count].getDtEnd();
row["from"] = from;
row["to"] = to;
if (from != null && to != null)
	row["interval"] = (intervals[count].getDtEnd().getTime() - intervals[count].getDtStart().getTime())/(1000*60*60);
count++;
return true;]]></method>
        </script-data-set>
